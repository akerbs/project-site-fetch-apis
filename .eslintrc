{
  "globals": {
    "expect": true,
    "angular": true,
    "AppConfig": true,
    "$": true,
    "$$": true,
    "jQuery": true,
    "$templateRequest": true,
    "$compile": true,
    "marked": true,
    "window": true,
    "document": true,
    "_": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-use-before-define": 0,
    "no-console": 0,
    "prettier/prettier": ["error"],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  }
}
